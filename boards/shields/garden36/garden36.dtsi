#include <physical_layouts.dtsi>
#include <dt-bindings/zmk/matrix_transform.h>

 / {
     chosen {
         zmk,kscan            = &kscan0;
         zmk,physical-layout  = &physical_layout0;
     };
     
    default_transform: keymap_transform_0 {
         compatible = "zmk,matrix-transform";
         columns    = <10>;
         rows       = <8>;
    
         // | SW1  | SW2  | SW3  | SW4  | SW5  |   | SW15 | SW11 | SW7  | SW4  | SW1  |
         // | SW6  | SW7  | SW8  | SW9  | SW10 |   | SW16 | SW12 | SW8  | SW5  | SW2  |
         // | SW11 | SW12 | SW13 | SW14 | SW15 |   | SW17 | SW13 | SW9  | SW6  | SW3  |
         //               | SW16 | SW17 | SW18 |   | SW18 | SW14 | SW10 |

        map = <
            RC(0,0) RC(0,1) RC(0,2) RC(0,3) RC(0,4)     RC(4,9) RC(4,8) RC(4,7) RC(4,6) RC(4,5)
            RC(1,0) RC(1,1) RC(1,2) RC(1,3) RC(1,4)     RC(5,9) RC(5,8) RC(5,7) RC(5,6) RC(5,5)
            RC(2,0) RC(2,1) RC(2,2) RC(2,3) RC(2,4)     RC(6,9) RC(6,8) RC(6,7) RC(6,6) RC(6,5)
                            RC(3,2) RC(3,3) RC(3,4)     RC(7,9) RC(7,8) RC(7,7)
        >;
    };
     physical_layout0: physical_layout_0 {
        compatible = "zmk,physical-layout";
        display-name = "Default Layout";

        kscan = <&kscan0>;
        transform = <&default_transform>;

        keys  //                     w   h    x    y     rot    rx    ry
            = <&key_physical_attrs 100 100    0  126       0     0     0>
            , <&key_physical_attrs 100 100  100   50       0     0     0>
            , <&key_physical_attrs 100 100  200    0       0     0     0>
            , <&key_physical_attrs 100 100  300   38       0     0     0>
            , <&key_physical_attrs 100 100  400   86       0     0     0>
            , <&key_physical_attrs 100 100    0  226       0     0     0>
            , <&key_physical_attrs 100 100  100  150       0     0     0>
            , <&key_physical_attrs 100 100  200  100       0     0     0>
            , <&key_physical_attrs 100 100  300  138       0     0     0>
            , <&key_physical_attrs 100 100  400  186       0     0     0>
            , <&key_physical_attrs 100 100    0  326       0     0     0>
            , <&key_physical_attrs 100 100  100  250       0     0     0>
            , <&key_physical_attrs 100 100  200  200       0     0     0>
            , <&key_physical_attrs 100 100  300  238       0     0     0>
            , <&key_physical_attrs 100 100  400  286       0     0     0>
            , <&key_physical_attrs 100 100  295  387    1000   345   437>
            , <&key_physical_attrs 100 100  400  415    2000   450   465>
            , <&key_physical_attrs 100 100  501  348    2500   551   398>
            , <&key_physical_attrs 100 100 1173  126       0     0     0>
            , <&key_physical_attrs 100 100 1073   50       0     0     0>
            , <&key_physical_attrs 100 100  973    0       0     0     0>
            , <&key_physical_attrs 100 100  873   38       0     0     0>
            , <&key_physical_attrs 100 100  773   86       0     0     0>
